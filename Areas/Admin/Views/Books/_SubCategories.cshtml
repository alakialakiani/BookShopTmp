@model IEnumerable<BookShopTmp.Models.ViewModels.TreeViewCategory>

    <ul>
        @foreach (var item in Model)
        {
            <li>
                <input type="checkbox" value="@item.CategoryId" name="CategoryId" /> @item.CategoryName
                @await Html.PartialAsync("_SubCategories", item.SubCategories)
            </li>
        }
    </ul>
